<% if (title) { %>
    <h1 class="login-title"><%= title %></h1>
    <% } else {
      res.redirect('/')
    } %>

<%- messages() %>

    <div class="account-info">
        <h2>You're logged in</h2>


        <% if (errors) { %>
            <ul class="notice">
           <% errors.array().forEach(error => { %>
             <li><%= error.msg %></li>
          <%  }) %>
           </ul>
          <% } %>
    </div>

